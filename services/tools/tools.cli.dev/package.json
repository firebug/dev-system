{
  "name": "tools.cli.dev",
  "version": "0.0.0",
  "private": true,
  "pm": "npm",
  "dependencies": {
	  "fs-extra": "~0.8.1",
    "colors": "^0.6.2",
    "commander": "^2.2.0",
    "q": "~1.0.0",
    "smi.cli": "0.x"    
  },
  "scripts": {
    "install": "./node_modules/.bin/smi install"
