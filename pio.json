    "upstream": {
        "packages": {
            "os-inception": [
                "../os.inception/services/*/*",
                "../devcomp/_upstream/*/*"
            ]
        },
        "catalogs": {
            "os-inception": {
                "uri": "http://assets.firebug.io/catalog/os-inception~int",
                "headers": {
                    "x-pio.catalog-key": "37a1043d-596a-4bd5-bf60-6d057b30dec2"
                }
            },
            "io-devcomp": {
                "uri": "http://pio-catalog.devcomp-i80fde72-3.vm.cadorn.github.pinf.me:8013/catalog/io.devcomp/063706ce21f32ab95ea0a942aa93c552eb3db9e2",
